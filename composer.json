{
    "name": "tamedia-adtec/jmspcd",
    "type": "symfony-bundle",
    "description": "A unified API for processing payments with Symfony. Based on jms/payment-core-bundle. Adapted to work with phpseclib/mcrypt_compat in order to phase out ext-mcrypt.",
    "keywords": ["payment"],
    "homepage": "http://jmspaymentcorebundle.readthedocs.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        },
        {
            "name": "Paulo Rodrigues Pinto",
            "email": "regularjack@gmail.com"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/schmittjoh/JMSPaymentCoreBundle/contributors"
        }
    ],
    "require": {
        "php": ">=7.2",
        "phpseclib/mcrypt_compat": "^1.0.8",
        "doctrine/common": "^2.9.0",
        "doctrine/dbal": "^2.8.0",
        "doctrine/orm": "^2.6.2",
        "symfony/browser-kit": "^4.1.6",
        "symfony/config": "^4.1.6",
        "symfony/dependency-injection": "^4.1.6",
        "symfony/event-dispatcher": "^4.1.6",
        "symfony/form": "^4.1.6",
        "symfony/options-resolver": "^4.1.6",
        "symfony/http-foundation": "^4.1.6",
        "symfony/http-kernel": "^4.1.6",
        "symfony/validator": "^4.1.6",
        "defuse/php-encryption": "^2.2.1",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4.3",
        "symfony/phpunit-bridge": "^4.1.6",
        "doctrine/doctrine-bundle": "^1.9.1",
        "sensio/framework-extra-bundle": "^5.2.1",
        "symfony/dom-crawler": "^4.1.6",
        "symfony/doctrine-bridge": "^4.1.6",
        "symfony/framework-bundle": "^4.1.6",
        "symfony/routing": "^4.1.6",
        "symfony/templating": "^4.1.6",
        "symfony/twig-bundle": "^4.1.6",
        "symfony/twig-bridge": "^4.1.6",
        "twig/twig": "^2.5.0",
        "matthiasnoback/symfony-config-test": "^4.0.0"
    },
    "autoload": {
        "psr-0": { "JMS\\Payment\\CoreBundle": "" }
    },
    "target-dir": "JMS/Payment/CoreBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
